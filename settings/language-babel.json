{
  ".source.js.jsx": {
    "editor": {
      "commentStart": "// ",
      "foldEndPattern": "^\\s*\\}|^\\s*\\]|^\\s*\\)",
      "increaseIndentPattern": "{[^}\"']*$|\\[[^\\]\"']*$|\\([^)\"']*$|case[\\s\\S]*:$|default:$",
      "decreaseIndentPattern": "^\\s*(\\s*/[*].*[*]/\\s*)*[}\\])]"
    }
  }
}
